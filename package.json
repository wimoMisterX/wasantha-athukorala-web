{
  "name": "wasantha-athukorala-web",
  "version": "1.0.0",
  "description": "Offical Website of Wasantha Athukorala Sole Propreitorship",
  "main": "index.js",
  "scripts": {
    "browserify:js": "browserify src/app/js/main.js -o src/assets/js/main.js ",
    "uglify:js": "uglifyjs src/assets/js/main.js -c -m -o src/assets/js/main.js",
    "csso:css": "csso src/app/css/main.css --output src/assets/css/main.css",
    "build": "npm run browserify:js && npm run uglify:js && npm run csso:css",
    "watch": "npm run build && watchify src/app/js/main.js -o src/assets/js/main.js -v",
    "start": "npm run build && NODE_ENV=development node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wimoMisterX/wasantha-athukorala-web.git"
  },
  "author": "wimoMisterX",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wimoMisterX/wasantha-athukorala-web/issues"
  },
  "homepage": "https://github.com/wimoMisterX/wasantha-athukorala-web#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "browserify": "^13.1.1",
    "config": "^1.26.1",
    "csso": "^2.3.0",
    "express": "^4.14.0",
    "flickity": "^2.0.5",
    "flickity-imagesloaded": "^2.0.0",
    "hogan": "^1.0.2",
    "image-preloader": "^1.1.1",
    "lodash": "^4.16.6",
    "mithril": "^0.2.5",
    "morgan": "^1.7.0",
    "nodemailer": "^2.6.4",
    "request": "^2.78.0",
    "serve-favicon": "^2.3.0",
    "uglify-js": "^2.7.4",
    "xoauth2": "^1.2.0"
  },
  "dev-dependencies": {
    "watchify": "^3.7.0"
  }
}
