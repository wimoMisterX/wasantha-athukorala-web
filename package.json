  },
  "homepage": "https://github.com/wimoMisterX/wasantha-athukorala-web#readme",
  "dependencies": {
    "browserify": "^13.1.1",
    "lodash": "^4.16.6",
    "mithril": "^0.2.5",
    "msx": "^0.4.1",
    "spserver": "^0.1.3",
    "stylus": "^0.54.5"
  }
}
