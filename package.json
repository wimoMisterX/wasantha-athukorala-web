{
  "name": "wimo-mister-x",
  "version": "1.0.0",
  "description": "Offical Website of Wasantha Athukorala Sole Propreitorship",
  "main": "index.js",
  "scripts": {
    "build": "browserify -t brfs src/app/main.js -o src/assets/js/main.js",
    "watch": "watchify -t brfs src/app/main.js -o src/assets/js/main.js -v",
    "build:prod": "npm run build && uglifyjs src/assets/js/main.js -c -m -o src/assets/js/main.js && csso src/assets/css/main.css --output src/assets/css/main.css",
    "start_dev": "NODE_ENV=dev node index.js",
    "start": "NODE_ENV=prod node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wimoMisterX/wasantha-athukorala-web.git"
  },
  "author": "wimoMisterX",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wimoMisterX/wasantha-athukorala-web/issues"
  },
  "homepage": "https://github.com/wimoMisterX/wasantha-athukorala-web#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "brfs": "^1.4.3",
    "browserify": "^13.1.1",
    "csso": "^2.3.0",
    "express": "^4.14.0",
    "flickity": "^2.0.5",
    "flickity-imagesloaded": "^2.0.0",
    "lodash": "^4.16.6",
    "mithril": "^0.2.5",
    "morgan": "^1.7.0",
    "nodemailer": "^2.6.4",
    "request": "^2.78.0",
    "serve-favicon": "^2.3.0",
    "uglify-js": "^2.7.4",
    "watchify": "^3.7.0"
  }
}
